#: pg_basebackup.c:133
#, c-format
msgid ""
"\n"
"Options controlling the output:\n"
msgstr ""
"\n"
"Opções que controlam a saída:\n"

#: pg_basebackup.c:134
#, c-format
msgid "  -D, --pgdata=DIRECTORY receive base backup into directory\n"
msgstr "  -D, --pgdata=DIRETÓRIO armazena a cópia de segurança base no diretório\n"

