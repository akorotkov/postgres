#: pg_basebackup.c:1220 pg_basebackup.c:1240 pg_basebackup.c:1247
#: pg_basebackup.c:1294
#, c-format
msgid "%s: out of memory\n"
msgstr "%s: memoria agotada\n"

#: pg_basebackup.c:1370
#, c-format
