#: pg_basebackup.c:472
#, c-format
msgid "%*s/%s kB (100%%), %d/%d tablespace %*s"
msgid_plural "%*s/%s kB (100%%), %d/%d tablespaces %*s"
msgstr[0] "%*s/%s kB (100%%), %d/%d tablespace %*s"
msgstr[1] "%*s/%s kB (100%%), %d/%d tablespaces %*s"

